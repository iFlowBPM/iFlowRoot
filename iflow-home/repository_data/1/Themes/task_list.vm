   #end
	<div class="table_inc" id="msg_table" style="display:$msg_display">
		<p>$notificationsMsg</p>
        <table class="item_list">
          <tr class="tab_header">
			<td></td>
			<td>$notes_from</td>
			<td>$notes_message</td>
